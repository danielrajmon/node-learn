    "@angular/core": "21.1.3",
    "@angular/forms": "21.1.3",
    "@angular/platform-browser": "21.1.3",
    "@angular/router": "21.1.3",
    "highlight.js": "11.11.1",
    "ngx-quill": "30.0.1",
    "quill": "2.0.3",
    "rxjs": "7.8.2",
    "tslib": "2.8.1"
  },
  "devDependencies": {
    "@angular/build": "^21.1.3",
    "@angular/cli": "^21.1.3",
    "@angular/compiler-cli": "^21.1.3",
    "jsdom": "^28.0.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
