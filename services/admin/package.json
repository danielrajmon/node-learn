{
  "name": "admin-service",
  "version": "1.0.0",
  "description": "Admin microservice for Node-Learn",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "start:dev": "ts-node src/main.ts",
    "start:watch": "nodemon --watch src --exec ts-node src/main.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "@node-learn/events": "file:../../packages/events",
    "@node-learn/messaging": "file:../../packages/messaging",
    "@nestjs/common": "11.1.13",
    "@nestjs/core": "11.1.13",
    "@nestjs/platform-express": "11.1.13",
    "@nestjs/typeorm": "11.0.0",
    "typeorm": "0.3.28",
    "pg": "8.18.0",
    "class-validator": "0.14.3",
    "class-transformer": "0.5.1",
    "jsonwebtoken": "9.0.3",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.2.1",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
